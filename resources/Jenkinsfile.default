// Container Slave
node(env.label) {
  stage('*checkout') {
    checkout scm
  }

  def conf = yaml.load(env._config)
  for(name in conf.stage) {
    stage(name) {
      //eachMap(conf[name], ret)
      conf[name].each { it.each {
        container(it.key) {
          def body = modular.eval2(it.key, it.value)
          body()
        }
      }}
    }
  }
}